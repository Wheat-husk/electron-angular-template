{
  "name": "electron-angular-template",
  "description": "This project was generated with [Angular CLI](https://github.com/angular/angular-cli) version 12.2.11.",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wheat-husk/electron-angular-template.git"
  },
  "author": {
    "name": "Wheat-husk",
    "email": "miaomimiya@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Wheat-husk/electron-angular-template/issues"
  },
  "homepage": "https://github.com/Wheat-husk/electron-angular-template#readme",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "npm-run-all -p ng:dev  electron:dev ",
    "build": "npm-run-all -p ngbuild:prod electron:build",
    "ng:dev": "ng serve --hmr -c dev",
    "ng:build": "ng build --base-href ./",
    "ngbuild:prod": "npm run ng:build -- -c production",
    "electron:tsc":"tsc -p tsconfig.serve.json",
    "electron:dev": "wait-on tcp:4200 && cross-env XDG_CURRENT_DESKTOP='serve' etan start -w",
    "electron:build": "cross-env XDG_CURRENT_DESKTOP='prod' etan build",
    "electron:local": "npm run ngbuild:prod && npx electron .",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run electron:tsc && npm run ngbuild:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "pretty-quick": "pretty-quick --staged",
    "prettier": "prettier -c --write '**/*'",
    "prepare": "husky install"
  },
  "private": true,
  "optionalDependencies": {
    "fsevents": "*"
  },
  "dependencies": {
    "electron-updater": "4.3.9",
    "fs-extra": "10.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/animations": "~12.2.0",
    "@angular/cli": "~12.2.11",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@etanjs/cli": "1.0.3",
    "@types/chai": "4.2.22",
    "@types/jasmine": "~3.8.0",
    "@types/jest": "27.0.2",
    "@types/mocha": "9.0.0",
    "@types/node": "12.20.33",
    "chai": "4.3.4",
    "conventional-changelog-cli": "2.1.1",
    "cross-env": "7.0.3",
    "electron": "13.6.0",
    "electron-builder": "22.13.1",
    "husky": "7.0.4",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mocha": "9.1.3",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "rxjs": "~6.6.0",
    "spectron": "15.0.0",
    "ts-node": "10.3.1",
    "tslib": "^2.3.0",
    "typescript": "~4.3.5",
    "wait-on": "6.0.0",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
